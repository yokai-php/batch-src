{
    "name": "yokai/batch-symfony-messenger",
    "description": "symfony/messenger bridge for yokai/batch",
    "keywords": ["batch", "job", "execution", "async", "message"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yann Eugon√©",
            "email": "eugone.yann@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/messenger": "^5.0|^6.0",
        "yokai/batch": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Yokai\\Batch\\Bridge\\Symfony\\Messenger\\": "src/"
        }
    },
    "require-dev": {
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Yokai\\Batch\\Tests\\Bridge\\Symfony\\Messenger\\": "tests/"
        }
    }
}
