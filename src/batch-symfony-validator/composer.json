{
    "name": "yokai/batch-symfony-validator",
    "description": "symfony/validator bridge for yokai/batch",
    "keywords": ["batch", "job", "processor", "validation"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yann Eugon√©",
            "email": "eugone.yann@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "symfony/validator": "^5.0|^6.0",
        "yokai/batch": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "Yokai\\Batch\\Bridge\\Symfony\\Validator\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Yokai\\Batch\\Tests\\Bridge\\Symfony\\Validator\\": "tests/"
        }
    }
}
