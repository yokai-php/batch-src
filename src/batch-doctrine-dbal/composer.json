{
  "name": "yokai/batch-doctrine-dbal",
  "description": "doctrine/dbal bridge for yokai/batch",
  "keywords": ["batch", "job", "execution", "dbal"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Yann Eugon√©",
      "email": "eugone.yann@gmail.com"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "doctrine/dbal": "^2.11",
    "yokai/batch": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Yokai\\Batch\\Bridge\\Doctrine\\DBAL\\": "src/"
    }
