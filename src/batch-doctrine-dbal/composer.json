  "require-dev": {
    "phpunit/phpunit": "^9.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Yokai\\Batch\\Tests\\Bridge\\Doctrine\\DBAL\\": "tests/"
    }
  }
