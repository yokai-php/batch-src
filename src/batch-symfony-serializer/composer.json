{
    "name": "yokai/batch-symfony-serializer",
    "description": "symfony/serializer bridge for yokai/batch",
    "keywords": ["batch", "job", "processor", "normalize", "denormalize"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Yann Eugon√©",
            "email": "eugone.yann@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/serializer": "^6.4|^7.0",
        "yokai/batch": "^0.5.0"
    },
    "autoload": {
        "psr-4": {
            "Yokai\\Batch\\Bridge\\Symfony\\Serializer\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Yokai\\Batch\\Tests\\Bridge\\Symfony\\Serializer\\": "tests/"
        }
    }
}
